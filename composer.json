{
    "name": "ngopibareng/phprosemirror",
    "description": "Work with ProseMirror JSON in PHP",
    "keywords": [
        "ngopibareng",
        "phprosemirror"
    ],
    "homepage": "https://github.com/ngopibareng/phprosemirror",
    "license": "The Unlicense",
    "type": "library",
    "authors": [
        {
            "name": "Ngopibareng",
            "email": "ngopibarengdotid@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "NgopiBareng\\Phprosemirror\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NgopiBareng\\Phprosemirror\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    }
}
